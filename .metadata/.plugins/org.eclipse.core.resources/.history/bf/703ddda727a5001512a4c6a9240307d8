package com.pruebas;

public class Niveles {
	int id;
	Carreras fkCarrera;
	int nivel;
	
	public Niveles() {
		super();
	}

	// Constructor para ingresar datos a la base
	public Niveles(Carreras fkCarrera, int nivel) {
		this.fkCarrera = fkCarrera;
		this.nivel = nivel;
	}

	// Constructor para consultar datos 
	public Niveles(int id, Carreras fkCarrera, int nivel) {
		this.id = id;
		this.fkCarrera = fkCarrera;
		this.nivel = nivel;
	}
	
	
	
	
}
